import java.util.Scanner;
public class pattern
{
    public static void main(String[]args)
    {
        int rows=5;
        for(int i=0;i<=rows;i++)
        {
            if(i==1){
                System.out.print(" 1");
            }
            else if(i==2){
                System.out.print("1  1");
            }
            else if(i==3){
                System.out.print("1 2 1");
            }
            System.out.println();
            
            }
            }
}
