import java.util.Scanner;
public class reverse
{
    public static void main (String[] args )
    {
        int n;
        Scanner Scanner = new Scanner(System.in);
        System.out.print("Enter the digit:");
        n = Scanner.nextInt();12
        int rev = 0;
        while(n!=0)
        {
            int rem = n%10;
            rev =rev*10+rem;
            n = n/10;
        }
        
        System.out.print(+rev+ " ");
        
        }
    }
