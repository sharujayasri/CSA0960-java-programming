import java.util.Scanner;
public class factors
{
    public static void main(String[]args)
    {
        Scanner Scanner=new Scanner (System.in);
        int n;
        System.out.print("Enter the n value:");
        n=Scanner.nextInt();
        int fact =0;
        for(int i=1;i<=n;i++)
        {
            if(n%i==0)
            {
                fact++;
            }
        }
            System.out.print(fact+ " ");
        }
    }

       
