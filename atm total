import java.util.Scanner;
public class primecomp
{
    public static void main(String[]args)
    {
       Scanner Scanner=new Scanner(System.in);
        int n1=4,d1=500,n2=10,d2=200,n3=5,d3=100,n4=15,d4=2000;
    int total =((n1*d1)+(n2*d2)+(n3*d3)+(n2*d4));
    System.out.print("total number of notes:"+total);
    }
}
